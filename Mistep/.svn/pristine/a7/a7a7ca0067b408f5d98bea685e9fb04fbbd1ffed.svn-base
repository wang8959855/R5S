- (IBAction)qqLogin:(id)sender
{
    UMSocialSnsPlatform *snsPlatform = [UMSocialSnsPlatformManager getSocialPlatformWithName:UMShareToQQ];
    snsPlatform.loginClickHandler(self,[UMSocialControllerService defaultControllerService],YES,^(UMSocialResponseEntity *response){
        if (response.responseCode == UMSResponseCodeSuccess) {
            [self addActityIndicatorInView:self.view labelText:NSLocalizedString(@"正在登录", nil) detailLabel:nil];
            NSDictionary *dict = [UMSocialAccountManager socialAccountDictionary];
            UMSocialAccountEntity *snsAccount = [dict valueForKey:@"qq"];
            NSDictionary *dic = @{@"user.password":snsAccount.usid,@"user.type":@"2"};
            [HCHCommonManager getInstance].userInfoDictionAry = nil;
            [[NSUserDefaults standardUserDefaults] removeObjectForKey:@"loginCache"];
            [[NSUserDefaults standardUserDefaults] synchronize];
            
            [[HCHCommonManager getInstance] setUserNickWith:response.thirdPlatformUserProfile[@"nickname"]];
            [[AFAppDotNetAPIClient sharedClient] globalDownloadWithUrl:response.thirdPlatformUserProfile[@"figureurl_qq_2"] Block:^(id responseObject, NSURL *filePath, NSError *error) {
                if (error) {
                    [self thirdLoginWithLoginParam:dic ];
                }else {
                    if (filePath) {
                        [[HCHCommonManager getInstance] setUserHeaderWith:[filePath lastPathComponent]];
                        [self thirdLoginWithLoginParam:dic ];
                    }
                }
            }];
            [[NSUserDefaults standardUserDefaults] setObject:dic forKey:kThirdPartLoginKey];
            [[NSUserDefaults standardUserDefaults] setObject:nil forKey:LastLoginUser_Info];
            [[NSUserDefaults standardUserDefaults] synchronize];
        }});
}
