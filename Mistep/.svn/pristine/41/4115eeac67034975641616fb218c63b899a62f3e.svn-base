#import <Foundation/Foundation.h>

@interface DayOverViewDataModel : NSObject

/**
 *  数据日期,格式为秒数 [NSDate dateWithTimeIntervalSince1970:seconds]可转化为date
 */
@property (unsafe_unretained, nonatomic) int timeSeconds;

/**
 *  全天总步数
 */
@property (unsafe_unretained, nonatomic) int steps;

/**
 *  全天里程，单位为米
 */
@property (unsafe_unretained, nonatomic) int meters;

/**
 *  全天消耗卡路里
 */
@property (unsafe_unretained, nonatomic) int costs;

/**
 *  全天运动时长
 */
@property (unsafe_unretained, nonatomic) int activityTime;

/**
 *  全天静坐时长
 */
@property (unsafe_unretained, nonatomic) int calmTime;

/**
 *  全天运动消耗
 */
@property (unsafe_unretained, nonatomic) int activityCosts;

/**
 *  全天静坐消耗
 */
@property (unsafe_unretained, nonatomic) int calmCosts;

@end
