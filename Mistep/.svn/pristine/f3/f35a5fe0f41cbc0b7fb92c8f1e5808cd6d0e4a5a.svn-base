#import <Foundation/Foundation.h>

@interface DayOverViewDataModel : NSObject

/**
 *  数据日期,格式为秒数 [NSDate dateWithTimeIntervalSince1970:seconds]可转化为date
 */
@property (unsafe_unretained, nonatomic) uint timeSeconds;

/**
 *  全天总步数
 */
@property (unsafe_unretained, nonatomic) uint steps;

/**
 *  全天里程，单位为米
 */
@property (unsafe_unretained, nonatomic) uint meters;

/**
 *  全天消耗卡路里
 */
@property (unsafe_unretained, nonatomic) uint costs;

/**
 *  全天运动时长
 */
@property (unsafe_unretained, nonatomic) uint activityTime;

/**
 *  全天静坐时长
 */
@property (unsafe_unretained, nonatomic) uint calmTime;

/**
 *  全天运动消耗
 */
@property (unsafe_unretained, nonatomic) uint activityCosts;

/**
 *  全天静坐消耗
 */
@property (unsafe_unretained, nonatomic) uint calmCosts;

@end
